{
  "name": "my-project",
  "version": "0.0.3",
  "description": "electron-demo",
  "author": "liu",
  "private": true,
  "main": "src/main/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ryuuboy/electron-demo"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "ele": "wait-on http://localhost:9080 && electron .",
    "dev": "cross-env NODE_ENV=development concurrently \"npm run serve\" \"npm run ele\"",
    "dist": "vue-cli-service build",
    "build": "cross-env NODE_ENV=production vue-cli-service build && electron-builder --win --config --publish always",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "electron-updater": "^6.2.1",
    "element-ui": "^2.15.14",
    "vue": "^2.6.11",
    "vue-router": "^3.2.0",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.4.6",
    "@vue/cli-plugin-eslint": "~4.4.6",
    "@vue/cli-plugin-router": "~4.4.6",
    "@vue/cli-plugin-vuex": "~4.4.6",
    "@vue/cli-service": "~4.4.6",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^31.2.1",
    "electron-builder": "^24.13.3",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "prettier": "^1.19.1",
    "vue-template-compiler": "^2.6.11",
    "wait-on": "^7.2.0"
  },
  "homepage": "./",
  "build": {
    "appId": "com.liu.myProject",
    "productName": "electron-demo",
    "directories": {
      "output": "output"
    },
    "asar": true,
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "perMachine": true,
      "deleteAppDataOnUninstall": true
    },
    "win": {
      "icon": "dist/icon.ico"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "ryuuboy",
        "repo": "electron-demo",
        "token": "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",
        "releaseType": "release"
      }
    ]
  }
}
